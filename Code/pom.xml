    <build>
        <pluginManagement>
            <plugins>
                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-compiler-plugin</artifactId>
                    <version>2.3.2</version>
                </plugin>
                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-resources-plugin</artifactId>
                    <version>2.4.3</version>
                </plugin>
                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-surefire-plugin</artifactId>
                    <version>2.5</version>
                </plugin>
                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-jar-plugin</artifactId>
                    <version>2.3.1</version>
                    <configuration>
                        <archive>
                            <compress>false</compress>
                        </archive>
                    </configuration>
                </plugin>
                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-war-plugin</artifactId>
                    <version>2.1</version>
                    <configuration>
                        <archive>
                            <compress>false</compress>
                        </archive>
                        <archiveClasses>true</archiveClasses>
                    </configuration>
                </plugin>
                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-dependency-plugin</artifactId>
                    <version>2.1</version>
                </plugin>
                <plugin>
                    <groupId>org.codehaus.mojo</groupId>
                    <artifactId>jaxb2-maven-plugin</artifactId>
                    <version>1.3</version>
                </plugin>
                <plugin>
                    <groupId>org.apache.maven.plugins</groupId>
                    <artifactId>maven-antrun-plugin</artifactId>
                    <version>1.6</version>
                    <dependencies>
                        <dependency>
                            <groupId>org.aspectj</groupId>
                            <artifactId>aspectjtools</artifactId>
                            <version>${aspectj.version}</version>
                        </dependency>
                        <dependency>
                            <groupId>net.sf.jasperreports</groupId>
                            <artifactId>jasperreports</artifactId>
                            <version>3.7.6</version>
                        </dependency>
                    </dependencies>
                    <executions>
                        <execution>
                            <id>compile-aspects</id>
                            <goals>
                                <goal>run</goal>
                            </goals>
                            <configuration>
                                <target>
                                    <taskdef resource="org/aspectj/tools/ant/taskdefs/aspectjTaskdefs.properties"
                                             classpathref="maven.plugin.classpath"/>
                                    <iajc target="${project.build.java.target}"
                                          source="${project.build.java.target}"
                                          encoding="${project.build.sourceEncoding}"
                                          classpathref="maven.compile.classpath"
                                          XterminateAfterCompilation="true"
                                          debug="true"
                                          verbose="true"
                                          srcdir="${project.build.sourceDirectory}"
                                          destdir="${project.build.outputDirectory}">
                                        <include name="**/*.aj"/>
                                        <exclude name="**/*.java"/>
                                    </iajc>
                                </target>
                            </configuration>
                        </execution>
                        <execution>
                            <id>test-compile-aspects</id>
                            <goals>
                                <goal>run</goal>
                            </goals>
                            <configuration>
                                <target unless="maven.test.skip">
                                    <taskdef resource="org/aspectj/tools/ant/taskdefs/aspectjTaskdefs.properties"
                                             classpathref="maven.plugin.classpath"/>
                                    <iajc target="${project.build.java.target}"
                                          source="${project.build.java.target}"
                                          encoding="${project.build.sourceEncoding}"
                                          classpathref="maven.test.classpath"
                                          XterminateAfterCompilation="true"
                                          debug="true"
                                          verbose="true"
                                          srcdir="${project.build.testSourceDirectory}"
                                          destdir="${project.build.testOutputDirectory}">
                                        <include name="**/*.aj"/>
                                        <exclude name="**/*.java"/>
                                    </iajc>
                                </target>
                            </configuration>
                        </execution>
                        <execution>
                            <id>compile-jasperreports</id>
                            <goals>
                                <goal>run</goal>
                            </goals>
                            <configuration>
                                <target>
                                    <taskdef name="jrc"
                                             classname="net.sf.jasperreports.ant.JRAntCompileTask"
                                             classpathref="maven.plugin.classpath"/>

                                    <property name="jrc.srcdir" value="${basedir}/src/main/webapp/WEB-INF/reports"/>
                                    <property name="jrc.destdir" value="${project.build.directory}/${project.build.finalName}/WEB-INF/reports"/>
                                    <property name="jrc.tempdir" value="${project.build.directory}/jasper-temp"/>

                                    <mkdir dir="${jrc.destdir}"/>
                                    <mkdir dir="${jrc.tempdir}"/>

                                    <jrc srcdir="${jrc.srcdir}"
                                         destdir="${jrc.destdir}"
                                         tempdir="${jrc.tempdir}"
                                         keepjava="true"/>
                                </target>
                            </configuration>
                        </execution>
                    </executions>
                </plugin>
            </plugins>
        </pluginManagement>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <configuration>
                    <encoding>${project.build.sourceEncoding}</encoding>
