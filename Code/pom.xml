                <configuration>
                    <forkMode>once</forkMode>
                    <argLine>"-javaagent:${spring.instrument.path}"</argLine>
                    <systemPropertyVariables>
                        <derby.system.home>${derby.system.home}</derby.system.home>
                    </systemPropertyVariables>
                </configuration>
                <dependencies>
                    <dependency>
                        <groupId>org.springframework</groupId>
